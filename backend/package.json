{
  "name": "collaborative-playlist-backend",
  "version": "1.0.0",
  "description": "Backend for collaborative playlist manager",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:api": "./test-api.sh",
    "test:errors": "node test-error-handling.js",
    "docs": "curl -s http://localhost:5000/api | jq '.'",
    "health": "curl -s http://localhost:5000/health | jq '.'",
    "data:check": "node src/utils/dataConsistency.js check",
    "data:cleanup": "node src/utils/dataConsistency.js cleanup",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.2",
    "spotify-web-api-node": "^5.0.2",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "playlist",
    "collaborative",
    "music",
    "real-time",
    "spotify",
    "lastfm"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
